{
    "name": "laravel-boost/cline",
    "description": "Cline agent for Laravel Boost - MCP support for CLINE in Laravel projects",
    "keywords": ["laravel", "boost", "cline", "mcp", "ai", "development"],
    "license": "MIT",
    "authors": [
        {
            "name": "shenjian8",
            "email": "shenjain8@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "laravel/boost": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\BoostCline\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\BoostCline\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravel\\BoostCline\\ClineServiceProvider"
            ]
        }
    }
}
